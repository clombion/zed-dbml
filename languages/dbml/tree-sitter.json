{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tree_sitter_dbml",
      "camelcase": "TreeSitterDbml",
      "title": "TreeSitterDbml",
      "scope": "source.tree_sitter_dbml",
      "file-types": [
        "tree_sitter_dbml"
      ],
      "injection-regex": "^tree_sitter_dbml$",
      "class-name": "TreeSitterTreeSitterDbml"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for DBML (Database Markup Language)",
    "authors": [
      {
        "name": "clombion"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-tree_sitter_dbml"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}